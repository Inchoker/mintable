{
  "openapi": "3.0.2",
  "info": {
    "title": "Mintable API",
    "version": "1.0.0"
  },
  "servers": [
    {
      "url": "https://73q1g05wo2.execute-api.us-east-1.amazonaws.com"
    }
  ],
  "paths": {
    "/mintable": {
      "get": {
        "description": "Get nft by name",
        "parameters": [
          {
            "name": "name",
            "in": "query",
            "description": "name of the nft",
            "required": true,
            "schema": {
              "type": "string",
              "default": "poo",
            }
          }
        ],
        "responses": {
          "200": {
            "description": "OK",
            "schema": {
              "type": "object",
              "properties": {
                "nft": {
                  "type": "object"
                }

              }
            }
          }
        }
      },
      "post": {
        "description": "Create nft",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "type": "object",
                "default": {
                  "name": "a",
                  "desc": "b"
                 ,
                  "img": "c"
                },
                "property": {
                  "name": {
                    "type": "string",
                  },
                  "desc": {
                    "type": "string",
                  },
                  "img": {
                    "type": "string",
                  }
                }
              }}}}
            }
          }
  }
